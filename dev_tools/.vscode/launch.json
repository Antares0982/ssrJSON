        {
            "name": "(gdb) Launch C test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/ssrjson_test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "sourceFileMap": {
                "/build": "${workspaceFolder}/debug_source"
            },
            "externalConsole": false,
