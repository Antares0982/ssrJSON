{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debug: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "python": "${workspaceFolder}/.nix-pyenv/bin/python",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/build"
            }
        },
        {
            "name": "(gdb) Launch clang",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/.nix-pyenv-llvm/debugLLVM/bin/llc",
            "args": [
                "-O3",
                "ssrjson.ll"
            ],
            "sourceFileMap": {
                "/build": "${workspaceFolder}/debug_source"
            },
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "enable gdb pretty printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "set disassembly flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerArgs": "-x ${workspaceFolder}/dev_tools/.gdbinit",
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/.nix-pyenv/bin/python",
            "args": [
                "${file}"
            ],
            "sourceFileMap": {
                "/build": "${workspaceFolder}/debug_source"
            },
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PYTHONPATH",
                    "value": "${workspaceFolder}/build"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "enable gdb pretty printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "set disassembly flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerArgs": "-x ${workspaceFolder}/dev_tools/.gdbinit",
            "preLaunchTask": "CMake: build with last config"
        },
        {
            "name": "(gdb) Launch test/all_test.py",
            "type": "cppdbg",
            "request": "launch",
            "program": "/usr/bin/env",
            "args": [
                "-S",
                "bash",
                "${workspaceFolder}/dev_tools/run_tests.sh"
            ],
            "sourceFileMap": {
                "/build": "${workspaceFolder}/debug_source"
            },
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
